main_cefi_cobalt_access <- function() {
cat("Starting CEFI Regional MOM6 COBALT chlorophyll access...\n\n")
# Access CEFI COBALT 3D chlorophyll data
cefi_data <- access_cefi_cobalt_chlorophyll()
if (is.null(cefi_data)) {
